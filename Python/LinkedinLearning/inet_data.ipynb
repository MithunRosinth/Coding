    "# Requesting data from web\n",
    "\n",
    "import urllib.request\n",
    "\n",
    "gm=urllib.request.urlopen(\"https://www.google.com\")\n",
    "# checking for response status code\n",
    "print(\"result code: \"+ str(gm.getcode()))\n",
    "\n",
    "# read some data and print it\n",
    "data=gm.read()\n",
    "print(data)"
   ]
